{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended", 
        "plugin:prettier/recommended",
        "plugin:flowtype/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "plugins": [
        "prettier",
        "flowtype"
    ],
    "rules": {
        "no-console": "off",
        "prettier/prettier": "error"
    }
}